{
  "name": "lynx-keymap",
  "displayName": "Lynx Keymap",
  "description": "Available now: VSCode, Cursor AI, Windsurf & Trae AI. + (Firebase Studio) & (+AI). ",
  "version": "2.0.1",
  "publisher": "bastndev",
  "license": "MIT",
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Keymaps",
    "Programming Languages",
    "AI",
    "Debuggers",
    "Chat"
  ],
  "keywords": [
    "keybindings",
    "keymap",
    "Vscode",
    "AI",
    "Cursor-AI",
    "Trae",
    "Windsurf",
    "Firebase studio",
    "Keybinding",
    "Keybindings",
    "shortcuts",
    "2025",
    "Windows",
    "Linux",
    "MacOS"
  ],
  "icon": "assets/icon.png",
  "galleryBanner": {
    "color": "#0a0a0a",
    "theme": "dark"
  },
  "homepage": "https://github.com/bastndev/Lynx-Keymap#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/bastndev/Lynx-Keymap"
  },
  "bugs": {
    "url": "https://github.com/bastndev/Lynx-Keymap/issues"
  },
  "author": {
    "name": "Gohit Bastian",
    "email": "gobasdev@gmail.com",
    "url": "https://www.bastndev.com"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lynx-keymap.generateAICommit",
        "title": "Generate AI Commit Message"
      },{
        "command": "lynx-keymap.executeAIPopup",
        "title": "Open AI Chat Interface"
      },{
        "command": "lynx-keymap.openAIChat",
        "title": "Open AI Chat"
      },{
        "command": "lynx-keymap.createNewAISession",
        "title": "Create New AI Session"
      },{
        "command": "lynx-keymap.showAIHistory",
        "title": "Show AI Chat History"
      },{
        "command": "lynx-keymap.attachAIContext",
        "title": "Attach Context to AI Chat"
      },{
        "command": "lynx-keymap.toggleInlineSuggest",
        "title": "Toggle Inline Suggestions (AI Autocomplete)"
      },
      {
        "command": "lynx-keymap.cycleIconColor",
        "title": "Cycle Icon Color (Red → Blue → Default)"
      },{
        "command": "lynx-keymap.executeColorAndAgentMacro",
        "title": "Execute Color Change + Agent Mode Toggle Macro"
      },
      {
        "command": "lynx-keymap.toggleGreenMode",
        "title": "Toggle Lynx Green Mode"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+1",
        "mac": "cmd+1",
        "command": "workbench.view.explorer"
      },{
        "key": "ctrl+2",
        "mac": "cmd+2",
        "command": "workbench.view.scm"
      },{
        "key": "ctrl+3",
        "mac": "cmd+3",
        "command": "workbench.view.extensions"
      },
      

      
      {
        "key": "alt+c",
        "mac": "alt+c",
        "command": "explorer.newFile"
      },{
        "key": "alt+v",
        "mac": "alt+v",
        "command": "explorer.newFolder"
      },{
        "key": "ctrl+tab",
        "mac": "cmd+tab",
        "command": "workbench.action.toggleSidebarVisibility"
      },


      {
        "key": "alt+1",
        "mac": "alt+1",
        "command": "git.stageAll",
        "when": "scmProvider == git"
      },{
        "key": "alt+2",
        "mac": "alt+2",
        "command": "lynx-keymap.generateAICommit",
        "when": "scmProvider == git"
      },{
        "key": "alt+3",
        "mac": "alt+3",
        "command": "git.unstageAll",
        "when": "scmProvider == git"
      },{
        "key": "alt+4",
        "mac": "alt+4",
        "command": "git.pull",
        "when": "scmProvider == git"
      },

      {
        "key": "alt+enter",
        "mac": "alt+enter",
        "command": "git.commit",
        "when": "scmProvider == git"
      },{
        "key": "ctrl+alt+enter",
        "mac": "cmd+alt+enter",
        "command": "git.push",
        "when": "scmProvider == git"
      },



      {
        "key": "alt+f",
        "mac": "alt+f",
        "command": "editor.action.formatDocument"
      },{
        "key": "ctrl+alt+f",
        "mac": "cmd+alt+f",
        "command": "editor.action.formatSelection.multiple"
      },{
        "key": "shift+alt+f",
        "mac": "shift+alt+f",
        "command": "editor.action.organizeImports"
      },



      {
        "key": "alt+q",
        "mac": "alt+q",
        "command": "workbench.debug.action.toggleRepl"
      },{
        "key": "alt+w",
        "mac": "alt+w",
        "command": "workbench.action.terminal.toggleTerminal"
      },{
        "key": "alt+e",
        "mac": "alt+e",
        "command": "gitlens.showGraph"
      },{
        "key": "alt+e",
        "mac": "alt+e",
        "command": "workbench.action.closePanel"
      },

      

      {
        "key": "ctrl+[Backquote]",
        "mac": "cmd+[Backquote]",
        "command": "lynx-keymap.executeAIPopup"
      },{
        "key": "shift+tab",
        "mac": "shift+tab",
        "command": "lynx-keymap.openAIChat"
      },

      
      {
        "key": "alt+a",
        "mac": "alt+a",
        "command": "lynx-keymap.createNewAISession"
      },{
        "key": "alt+s",
        "mac": "alt+s",
        "command": "lynx-keymap.showAIHistory"
      },{
        "key": "alt+d",
        "mac": "alt+d",
        "command": "lynx-keymap.attachAIContext"
      },
      

      {
        "key": "ctrl+shift+alt+12",
        "mac": "cmd+shift+alt+12",
        "command": "workbench.action.chat.toggleAgentMode"
      },{
        "key": "ctrl+shift+alt+11",
        "mac": "ctrl+shift+alt+11",
        "command": "lynx-keymap.cycleIconColor"
      },

      {
        "key": "alt+x",
        "mac": "alt+x",
        "command": "workbench.action.chat.openModelPicker"
      },{
        "key": "alt+z",
        "mac": "alt+z",
        "command": "lynx-keymap.executeColorAndAgentMacro"
      },{
        "key": "shift+escape",
        "mac": "shift+escape",
        "command": "workbench.action.toggleMaximizedAuxiliaryBar"
      },


      {
        "key": "ctrl+enter",
        "mac": "cmd+enter",
        "command": "-github.copilot.generate"
      },{
        "key": "alt+enter",
        "mac": "alt+enter",
        "command": "inlineChat.close",
        "when": "inlineChatFocused"
      },



      {
        "key": "alt+p",
        "mac": "alt+p",
        "command": "workbench.action.debug.start"
      },{
        "key": "alt+o",
        "mac": "alt+o",
        "command": "workbench.action.debug.restart"
      },{
        "key": "alt+i",
        "mac": "alt+i",
        "command": "workbench.action.debug.stop"
      },{
        "key": "ctrl+alt+p",
        "mac": "cmd+alt+p",
        "command": "lynx-keymap.toggleGreenMode"
      },



      {
        "key": "alt+l",
        "mac": "alt+l",
        "command": "git.viewChanges"
      },{
        "key": "alt+m",
        "mac": "alt+m",
        "command": "markdown.showPreviewToSide"
      },


      {
        "key": "ctrl+j",
        "mac": "cmd+j",
        "command": "workbench.action.openSettings"
      },{
        "key": "ctrl+,",
        "mac": "cmd+,",
        "command": ""
      },{
        "key": "alt+j",
        "mac": "alt+j",
        "command": "workbench.action.openSettingsJson"
      },


      {
        "key": "ctrl+/",
        "mac": "cmd+/",
        "command": "editor.action.commentLine"
      },{
        "key": "alt+/",
        "mac": "alt+/",
        "command": "editor.action.blockComment"
      },


      {
        "key": "alt+shift+up",
        "mac": "alt+shift+up",
        "command": "editor.action.copyLinesUpAction"
      },{
        "key": "alt+shift+down",
        "mac": "alt+shift+down",
        "command": "editor.action.copyLinesDownAction"
      },{
        "key": "ctrl+up",
        "mac": "cmd+up",
        "command": "editor.action.insertCursorAbove"
      },{
        "key": "ctrl+down",
        "mac": "cmd+down",
        "command": "editor.action.insertCursorBelow"
      },


      {
        "key": "alt+escape",
        "mac": "alt+escape",
        "command": "lynx-keymap.toggleInlineSuggest"
      },{
        "key": "ctrl+backspace",
        "mac": "cmd+backspace",
        "command": "deleteWordLeft"
      },{
        "key": "ctrl+delete",
        "mac": "cmd+delete",
        "command": "workbench.action.closeFolder"
      }
    ]
  }
}